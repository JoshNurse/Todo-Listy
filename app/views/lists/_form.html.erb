<%= form_for(@list) do |f| %>
  <% if @list.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@list.errors.count, "error") %> prohibited this list from being saved:</h2>

      <ul>
      <% @list.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

	<table id="userSession">
		<tr>
			<td><%= f.label :name %></td>
			<td><%= f.text_field :name, :id => "focusMe" %></td>
		</tr>
	</table>

  <div class="actions">
    <%= submit_tag "Submit", :disable_with => "Saving..." %> or <%= link_to 'cancel', :back %>
  </div>
<% end %>